




  

  {%- assign collection = collections[section.settings.collection] -%}



  {%- assign product_limit = section.settings.grid | times: section.settings.rows -%}


 <!-- newlrelease section start here -->
  <section id="newlrelese" class="text-center">
    <div class="container">
      <div class="row justify-content-center">
        <h1 class=" Organetto_UltraBold text-center text-white mt-5 pt-5 gr-text"> {{section.settings.title}} </h1>
        <h4 class="gr-text mb-5 pb-5 ">Our Newest & Best Selling Products Available Now!</h4>
     
       <div class="product-wrap-slide" >

    {% for product in collection.products limit: product_limit %}
       
      <div class="">
        
        <a href="{{product.url}}"> 
          <div class="product-wrapper" >
			<div class="product-badge-wrapper" >
              {%if product.metafields.product.isthisnew%}
                 <img loading="lazy" class="productbadge" src="{{"newproductbadge.png" | file_url}}"> 
              {%endif%}
             <img loading="lazy" class="product-img" src="{{product.metafields.product.replacement_img}}">
            </div>

           
            <h3 class="product-title gr-text" > {{product.metafields.product.carousel_replacement_title}}</h3>
            </div>
          </a>
        
      </div>
      
   {% endfor %}
        </div>
      </div>
      <button style="visibility: hidden;" class="gr-btn px-5 py-2 mt-2 mb-2 btn text-white  " onclick="javascript:location.href='collections/all-products'">SHOP NOW</button> 
    </div>
  </section>


   <!-- fullbg-test-overhealth-section starts here ....  -->
    <div class="fullbg-test-overhealth-section ">
        <div class="container-fluid ">
            <div class="row ">
                <div class="col-12 px-0">
                    <!-- <img src="./images/reinventuherosect.png" alt="" width="100%" height="100%"> -->
                </div>
            </div>
        </div>

        <!-- test-overhealth-section starts here ....  -->
        <div class="container-fluid test-overhealth-section py-5 ">
            <div class="row text-center pt-5 pt-md-0 ">
                <div class="col-10 mx-auto whychoose-section pt-5 pt-md-0">
                    <h2 class="text-white fw-light pt-5 mt-5 pt-md-0 mt-md-0">Why Choose <span class="fw-bold"> Taste
                            Over Health</span> <br>
                        When You Can Have Both?</h2>
                    <hr class="w-25 mx-auto text-white">
                </div>
                <div>
                    <div class="text-center">
                        
                      <div class="fold-wrapper">
                      	<div class="slide-o">
                          <img class="mx-auto" src="{{"tasteit.png" | file_url}}"  alt="/electriciconpng">
                        <h3 class="text-white my-4 fw-light"> <span class="fw-bold"> Taste </span> It.</h2>
                            <div class="text-white col-10 col-md-6 col-xl-6 mx-auto">
                                <p class="text-white" >Our line of products are great for your everyday health, but even better for your tastebuds. Thank us later.
                                  </p>
                            </div>
                           
                      </div>
                        <div class="slide-o">
                          <img class="mx-auto" src="{{"strike.png" | file_url}}"  alt="/electriciconpng">
                        <h3 class="text-white my-4 fw-light"> <span class="fw-bold"> Feel </span> It.</h2>
                            <div class="text-white col-10 col-md-6 col-xl-6 mx-auto">
                                <p class="text-white" >Clinically dosed health supplements guaranteed to give you that buzz of great health
                                    you’re
                                    looking
                                    for.</p>
                            </div>
                          
                      </div>
                        <div class="slide-o">
                          <img class="mx-auto" src="{{"heart-fold.png" | file_url}}"  alt="/electriciconpng">
                        <h3 class="text-white my-4 fw-light"> <span class="fw-bold"> love </span> It.</h2>
                            <div class="text-white col-10 col-md-6 col-xl-6 mx-auto">
                                <p class="text-white"> Our 5,000+ ReinventU customers can’t help but come back for more. We hope the same for you!</p>
                            </div>
                           
                      </div>
                      </div>
                       <button onclick="javascript:window.location='/collections/health-suppliments'" class=" mt-4 test-overhealth-section-button">
                                <span class="fw-bold"> Shop Now! </span>
                            </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
