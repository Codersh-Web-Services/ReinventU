                    {% for value in page.metafields.faq.question %}
                    <div class="accordion-item">
                        <h2 class="accordion-header" id="{{variable1}}">
                           <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapse{{variable1}}" aria-expanded="true" aria-controls="collapseOne">
